{
  "name": "demo",
  "version": "0.1.0",
  "description": "Automation of android application",
  "scripts": {
    "android-test": "cd android && npm run test",
    "ios-test": "cd ios && npm run test",
    "test": "npm-run-all ios-test android-test --continue-on-error",
    "android-login-test": "cd android && npm run login-test",
    "ios-login-test": "cd ios && npm run login-test",
    "login-test": "npm-run-all ios-login-test android-login-test --continue-on-error"
  },
  "dependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "appium": "^2.2.2",
    "cross-env": "^5.2.0",
    "npm-run-all": "4.1.5"
  }
}
